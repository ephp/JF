{% include 'ClaimsHBundle:Tabellone:commons/repo_brick.html.twig' with {'span_label': 3, 'label': 'Relazione ex adverso', 'span_value': 9, 'value': entity.relazioneExAdverso} %}
{% include 'ClaimsHBundle:Tabellone:commons/repo_brick.html.twig' with {'span_label': 3, 'label': 'Medico Legale 1', 'span_value': 9, 'value': entity.MedicoLegale1} %}
{% include 'ClaimsHBundle:Tabellone:commons/repo_brick.html.twig' with {'span_label': 3, 'label': 'Medico Legale 2', 'span_value': 9, 'value': entity.MedicoLegale2} %}
