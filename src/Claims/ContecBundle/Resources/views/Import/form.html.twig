{% extends '::base.html.twig' %}

{% block body -%}
<div class="page-header">
    <div class="icon">
        <span class="ico-cog"></span>
    </div>
    <h1>Importazione <small>da Contec</small></h1>
</div>
<div class="row-fluid">
    <div class="span6">
        <div class="block">
            <label for="mr">Audit</label>
            <input id="mr" type="checkbox" value="1" {% if app.request.get('audit') %}checked="checked"{% endif %} />
            <input id="audit" type="hidden" value="{% if app.request.get('audit') %}{{ app.request.get('audit') }}{% else %}1{% endif %}" />
            {{ render(controller('EphpDragDropBundle:Render:uploadNf')) }}
{% javascripts  
    '@EphpUtilityBundle/Resources/public/js/check_sanitize_regexp.js'
    '@ClaimsContecBundle/Resources/public/js/form.js'
%}
    <script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}
        </div>
    </div>
</div>
{% endblock body -%}