                <textarea class="autogrow" pratica="{{ entity.slug }}" name="import" style="height: 350px;" placeholder="Incolla calendario da Ravinale"></textarea>
                <button class="btn" type="button" onclick="importaRavinale();">Importa</button>
            </form>
        </div>
    </div>
{% endif %}
