<tr>
    <th style="text-align: right;">{{ index }}</th>
    <td style="text-align: center;"><a href="{{ url('claims_hospital_pratica', {'slug': pratica.slug}) }}">{{ pratica.codice }}</a></td>
    <td><abbr title="{{ pratica.priorita.priorita }}">{{ pratica.priorita.priorita }}</abbr></td>
    <td>{{ pratica.claimant }}</td>
    <td>{{ pratica.nota }}</td>
</tr>
{% for entity in pratica.eventiOggi %}
<tr>
    <td style="text-align: right;">&nbsp;</td>
    <td title="{{entity.tipo.sigla}}">{% if entity.importante %}*{% else %}&nbsp;{% endif %}</td>
    <td>{{ entity.dataOra|date('d-m-Y') }}</td>
    <td>{{ entity.titolo }}</td>
    <td>{{ entity.note }}</td>
</tr>
{% endfor %}