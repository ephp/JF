<div class="page-header">
    {% if no_back is not defined %}
    <div class="icon">
        <span class="ico-cog"></span>
    </div>
    {% endif %}
    <h1>Report #{{ entity.number }} del {{ entity.data|date('d/m/Y')}} <small>{{ entity.pratica }}</small></h1>
</div>
{% include 'ClaimsHBundle:Tabellone:commons/repo_brick.html.twig' with {'span_label': 3, 'label': 'Copertura', 'span_value': 9, 'value': entity.copertura} %}
