<table class="table grid" cellpadding="0" cellspacing="0" width="100%">
    <thead>
        <tr>
            {% for td in tds %}
                {% include "ClaimsHBundle:Tabellone:index/th/"~td~".html.twig" %}
            {% endfor %}
        </tr>
    </thead>
    <tbody>
        {% set i = 0 %}
        {% if app.request.get('page') %}
            {% set i = (app.request.get('page') - 1) * 50 %}
        {% endif %}
        {% for entity in pagination %}
            {% set i = i + 1 %}
            {% include "ClaimsHBundle:Tabellone:index/tr_nuova.html.twig" %}
        {% endfor %}
    </tbody>
</table>    
