{% include 'ClaimsHBundle:Tabellone:commons/repo_brick.html.twig' with {'span_label': 3, 'label': 'Valutazione responsabilita', 'span_value': 9, 'value': entity.valutazioneResponsabilita} %}
{% include 'ClaimsHBundle:Tabellone:commons/repo_brick.html.twig' with {'span_label': 3, 'label': 'Analisi danno', 'span_value': 9, 'value': entity.analisiDanno} %}
{% include 'ClaimsHBundle:Tabellone:commons/repo_brick.html.twig' with {'span_label': 3, 'label': 'Riserva', 'span_value': 9, 'value': entity.riserva} %}
{% include 'ClaimsHBundle:Tabellone:commons/repo_brick.html.twig' with {'span_label': 3, 'label': 'Possibile rivalsa', 'span_value': 9, 'value': entity.possibileRivalsa} %}
