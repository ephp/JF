{% if no_back is not defined %}
    <a href="{{ path('claims_hospital_report', {'slug': entity.pratica.slug}) }}" onclick="return loadAjax($(this));">
        <span class="label label-inverse">Torna indietro</span>
    </a>
{% endif %}